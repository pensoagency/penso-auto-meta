{
  "manifest_version": 2,

	"name": "PENSO Auto Meta",
	"description": "",
	"version": "0.1.0",

	"browser_action": {
		"default_title": "Auto Meta",
		"default_icon": "favicon.png",
		"default_popup": "index.html"
	},

	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"permissions": [
		"tabs",
		"contextMenus",
		"bookmarks",
		"<all_urls>"
	],

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["selection.js"],
			"run_at": "document_start",
			"all_frames": true
		}
	],

	"background": {
		"matches": ["<all_urls>"],
		"scripts": ["jquery-2.1.1.min.js", "background.js"],
		"matches": ["http://*/*", "https://*/*"],
		"persistent": true
	}

}
